<Window x:Class="Event_System.feedrec"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Event_System"
        mc:Ignorable="d"
        Title="feedrec" Background="#FFABE3D1" WindowStartupLocation="CenterScreen" Height="450" Width="800" ResizeMode="NoResize">
    <Grid>
        <Label Height="70" Width="500" VerticalAlignment="Top" Content="FEEDBACK RECORD" Foreground="Purple" FontWeight="ExtraBold" FontSize="35" Margin="199,23,101,0" />
        <DataGrid Name="DG" Width="700" Margin="50,111,50,95" />
        <Button x:Name="Back" FontWeight="ExtraBold" FontSize="16" Foreground="Purple" Content="Back" HorizontalAlignment="Left" Margin="414,361,0,0" VerticalAlignment="Top" Width="104" Click="Back_Click" Height="31"/>
        <Button x:Name="Show" FontWeight="ExtraBold" Background="MediumAquamarine" FontSize="16" Foreground="Purple" Content="Show" HorizontalAlignment="Left" Margin="239,361,0,0" VerticalAlignment="Top" Width="104" Click="Show_Click" Height="31"/>

    </Grid>
</Window>
